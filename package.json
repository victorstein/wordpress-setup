{
  "name": "wordpress-setup",
  "productName": "wordpress-setup",
  "version": "1.0.0",
  "description": "WordPress Setup automator",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": {
    "name": "ClockWorks Inc",
    "email": "email@email.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./icon.ico",
        "setupIcon": "./icon.ico",
        "win32metadata": {
          "requested-execution-level": "requireAdministrator",
          "ProductName": "WordPress-Setup",
          "CompanyName": "ClockWorks",
          "FileDescription": "WordPress Setup automator",
          "InternalName": "wordpress-setup",
          "OriginalFilename": "WordPressSetup.exe"
        }
      },
      "electronWinstallerConfig": {
        "name": "WordPress-Setup",
        "setupIcon": "./icon.ico",
        "icon": "./icon.ico",
        "author": "ClockWorks Inc",
        "exe": "WordPressSetup.exe",
        "description": "WordPress Setup automator",
        "iconUrl": "https://s1.wp.com/i/favicon.ico?v=1447321881"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "wordpress_setup",
            "setupIcon": "./icon.ico",
            "loadingGif": "./loading.gif",
            "iconUrl": "https://s1.wp.com/i/favicon.ico?v=1447321881"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/index.html",
                  "js": "./src/renderer.js",
                  "name": "main_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "adm-zip": "^0.4.13",
    "bootstrap": "^4.3.1",
    "electron-squirrel-startup": "^1.0.0",
    "mysql": "^2.17.1",
    "node-powershell": "^4.0.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-router-dom": "^5.1.2",
    "react-step-wizard": "^5.3.0",
    "reactstrap": "^8.0.1",
    "request-progress": "^3.0.0",
    "uniqid": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@electron-forge/cli": "^6.0.0-beta.45",
    "@electron-forge/maker-deb": "^6.0.0-beta.45",
    "@electron-forge/maker-rpm": "^6.0.0-beta.45",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.45",
    "@electron-forge/maker-zip": "^6.0.0-beta.45",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.45",
    "@marshallofsound/webpack-asset-relocator-loader": "^0.5.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.1.1",
    "css-loader": "^3.2.0",
    "electron": "9.4.0",
    "file-loader": "^4.2.0",
    "node-loader": "^0.6.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.2.0"
  }
}
